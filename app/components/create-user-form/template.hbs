<vbox class='create-user-form'>
  {{input
    value=email
    placeholder="email"}}
    <br>
  {{input
    type="password"
    placeholder="password"
    value=password}}
    <br>
  {{input
    type="password"
    placeholder="confirm-password"
    value=password-confirmation}}
    <br>
  <button {{action "createAccount"}}>
    Create Account
  </button>
</vbox>
